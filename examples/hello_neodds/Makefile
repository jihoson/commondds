MAKEFILE      = Makefile
LIBRARY_PATH = $(LD_LIBRARY_PATH)

####### Compiler, tools and options

CC            = gcc
CXX           = g++
DEFINES       = -D_LINUX_VER -DLIB_PROJECT
CFLAGS        = -m64 -pipe -O2 -Wall -W -D_REENTRANT -fPIC $(DEFINES)
CXXFLAGS      = -m64 -pipe -O2 -W -D_REENTRANT -fPIC $(DEFINES)
INCPATH       = -I. -I$(LIBRARY_PATH)/includes -I$(LIBRARY_PATH)/modules -Iidl
QMAKE         = /usr/lib/x86_64-linux-gnu/qt5/bin/qmake
DEL_FILE      = rm -f
CHK_DIR_EXISTS= test -d
MKDIR         = mkdir -p
COPY          = cp -f
COPY_FILE     = cp -f
COPY_DIR      = cp -f -R
INSTALL_FILE  = install -m 644 -p
INSTALL_PROGRAM = install -m 755 -p
INSTALL_DIR   = cp -f -R
DEL_FILE      = rm -f
SYMLINK       = ln -f -s
DEL_DIR       = rmdir
MOVE          = mv -f
TAR           = tar -cf
COMPRESS      = gzip -9f
LINK          = $(CC)
LFLAGS        = -m64 -Wl,-O1
#LIBS          = $(SUBLIBS) -L../ -lNeoDDS -lpthread 
LIBS          = $(SUBLIBS) -L/home/jiho/NeoDDS -lNeoDDS -lpthread 
AR            = ar cqs
RANLIB        = 
SED           = sed
STRIP         = strip

####### Output directory

OBJECTS_DIR   = ./

####### Files

SOURCES       = src/pub.cpp src/sub.cpp 
OBJECTS       = pub.o sub.o
QMAKE_TARGET  = pub sub 
DESTDIR       = #avoid trailing-slash linebreak
TARGET        = pub sub 


first: all
####### Implicit rules

.SUFFIXES: .o .c .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o "$@" "$<"

####### Build rules

$(TARGET):  $(OBJECTS)  
	$(LINK) $(LFLAGS) -o pub pub.o $(OBJCOMP) $(LIBS)
	$(LINK) $(LFLAGS) -o sub sub.o $(OBJCOMP) $(LIBS)
#$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJCOMP) $(LIBS)

all: $(TARGET)
	@echo NeoDDS PATH : $(LIBRARY_PATH)

clean: 
	-$(DEL_FILE) $(OBJECTS) $(TARGET)
	-$(DEL_FILE) *~ core *.core

####### Compile

$(OBJECTS): $(SOURCES) 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o pub.o src/pub.cpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o sub.o src/sub.cpp


####### Install

install:  FORCE

uninstall:  FORCE

FORCE:

